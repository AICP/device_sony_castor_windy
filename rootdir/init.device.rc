on boot
    # Panel backlight
    chown system system /sys/class/leds/lcd-backlight/brightness
    chmod 660 /sys/class/leds/lcd-backlight/brightness

    # Vibrator
    chown system system /sys/devices/i2c-2/2-0049/intensity

    # Touch
    chown system system /sys/devices/virtual/input/clearpad/wakeup_gesture
